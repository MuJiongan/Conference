<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.conference" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.conference.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.conference.AttendeeMenu"/>
        <activity android:name="com.example.conference.SignUp"/>
        <activity android:name="com.example.conference.seeAllEventsActivity"/>
        <activity android:name="com.example.conference.seeMyEventsActivity"/>
        <activity android:name="com.example.conference.SocialNetworking"/>
        <activity android:name="com.example.conference.viewContactListActivity"/>
        <activity android:name="com.example.conference.manageMyAccountActivity"/>
        <activity android:name="com.example.conference.OrganizerMenu"/>
        <activity android:name="com.example.conference.SpeakerMenu"/>
    </application>

</manifest>